<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
  </head>
  <body>
    <%- include('menu.html') %> 
    <h2 class = "ml-2 my-3 text-center">검색 결과</h2>
    <div class = "container">
      <ul class = "list-group">
        <% for(var i = 0; i < data.length; i++){ %>
          <li class = "list-group-item">
          <div class="card" style="width:100%;">
            <div class="card-header">
              <h4 class="card-title"><%= data[i].title %></h4>
            </div>
            <div class="card-body">
              <p class="card-text"><%= data[i].address %></p>
              <img src = "<%= data[i].path %>"  width = "200"/><p></p>   
              <p class="card-subtitle mb-2">등록일 : <%= data[i].date %></p>
              <a href="/edit/<%= data[i]._id %>" class="card-link">수정하기</a>
              <a href="/list/" class="card-link">목록</a>
            </div>
          </div>
        </li>
        <% } %>
      </ul>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>
  </body>
</html>